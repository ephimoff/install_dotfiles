#!/usr/bin/env bash
# shellcheck disable=SC1090
source "${HOME}/Projects/dotfiles/bin/echos.sh"

# Start
bot "Starting $(basename "$0")"

directory=~/Projects/dotfiles/preferences
files=$(find $directory -iname ".*" -maxdepth 1 -type f)


for file in "${files[@]}" ; do
    if [[ -f "$file" ]] ; then
        # shellcheck disable=SC1090
        source "$file"
    fi
done

# Finished
bot "$(basename "$0") complete."
