{
  "rules": {
    "no-todo": true,
    "terminology": true,
    "unexpanded-acronym": {
      // AB is ignore, ABC is recognized.
      "min_acronym_len": 3,
      // GREEEEEEN is ignore
      "max_acronym_len": 5,
      // OSS is ignore
      "ignore_acronyms": [
        "GIT",
        "AWS",
        "MSSQL",
        "LDAP"
      ]
    },
    "common-misspellings": {
      // Misspellings to be ignored (case-insensitive)
      "ignore": []
    },
    "diacritics": {
      // List of additional words
      "words": [
        "t√¢mia"
      ]
    },
    "stop-words": {
      // Load default dictionary (see dict.txt in the repository)
      "defaultWords": true,
      // Syntax elements to skip. Overrides the default
      "skip": [
        "Blockquote"
      ],
      // Extra words
      "words": [
        "etc.",
        "you can"
      ],
      // Excluded words
      "exclude": [
        "solution",
        "utilize",
        "period of time"
      ]
    }
  }
}